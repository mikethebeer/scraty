{
    "vsicons.presets.angular": true,
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/*.js": { "when": "$(basename).ts" },
      "**/**.js": { "when": "$(basename).tsx" },
      "**/*.js.map": true
    }
}
